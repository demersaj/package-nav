# Environment variables for pkg_nav.sh
# Apple Developer Company Name
COMPANY_NAME="My Team"

# Apple Developer Team ID
TEAM_ID="N12345ABC"

# Keychain profile name for notarization
NOTARY_PROFILE="my_notary_profile"

# JSON endpoint URL for fetching latest Navigator version info
JSON_URL="https://downloads.mycompany.com/myproduct/latest.json"

# S3 bucket config
S3_BUCKET="my-bucket"
S3_PREFIX="packages/"
S3_REGION="us-west-2"
#S3_ACL="public-read"  # if you want public downloads
